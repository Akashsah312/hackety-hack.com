ntroller::Base                        
  helper :all # include all helpers, a
ll the time                           
  protect_from_forgery # See ActionCon
troller::RequestForgeryProtection for
details
                                      
  # Scrub sensitive parameters from yo
ur log                                
  # filter_parameter_logging :password
  helper_method :current_user
         
