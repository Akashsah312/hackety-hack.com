    @watching = current_user.watchings.build(:watches_id => params[:watches_id])
    if @watching.save[5;6H flash[:notice] = "Now watching #{@watching.watches.username}"[6;6H redirect_to root_url
    else[8;6H flash[:error] = "Unable to watch #{@watching.watches.username}."[9;6H redirect_to root_url
    end
  end

  def destroy
    @watching = current_user.watchings.find(params[:id])
    @watching.destroy
    flash[:notice] = "You are no longer watching #{@watching.watches.username}"
    redirect_to current_user
  end
end
