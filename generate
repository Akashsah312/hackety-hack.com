[?1l>[34h[?25h[?1049l]0;steve@deadreckoning:~/src/hackety-hack.com[steve@deadreckoning hackety-hack.com]$ vim app/controllers/messages_controller.rb 
[?1049h[?1h=[1;56r[34l[34h[?25h[23m[24m[0m[H[J[?25l[56;1H"app/controllers/messages_controller.rb" 47L, 848C[1;1Hclass MessagesController < ApplicationController
  def index
    @messages = Message.all
  end

  def show
    @message = Message.find(params[:id])
  end

  def new
    @message = Message.new
  end

  def edit
    @message = Message.find(params[:id])
  end

  def create
    @message = Message.new(params[:message])
    @message.sender = current_user[22;4H if @message.save[23;6H flash[:notice] = 'Message was successfully created.'[24;6H redirect_to(@message)
    else[26;6H render :action => "new"
    end
  end

  def update
    @message = Message.find(params[:id])[33;4H if @message.update_attributes(params[:message])[34;6H flash[:notice] = 'Message was successfully updated.'[35;6H redirect_to(@message)
    else[37;6H render :action => "edit"
    end
  end

  def destroy
    @message = Message.find(params[:id])
    @message.destroy[45;4H redirect_to(messages_url)
  end
end
