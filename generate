[?1049h[?1h=[1;56r[34l[34h[?25h[23m[24m[0m[H[J[?25l[56;1H"app/controllers/users_controller.rb" 36L, 617C[1;1Hclass UsersController < ApplicationController

  layout "right"

  def new
    @user = User.new
  end

  def create
   @user = User.new(params[:user])
   if @user.save
     flash[:notice] = "Registration successful."
     redirect_to dashboard_url
   else
     render :action => 'new'
   end
  end

  def edit
    @user = current_user
  end

  def update
    @user = current_user
    if @user.update_attributes(params[:user])[26;6H flash[:notice] = "Successfully updated profile."[27;6H redirect_to root_url
    else[29;6H render :action => "edit"
    end
  end

  def show
    @user = User.find_by_username(params[:username])
  end
end
